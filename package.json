{
  "name": "fe-balance-debate",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://balance-debate.github.io/fe-balance-debate/",
  "scripts": {
    "dev": "next dev",
    "dev:https": "next dev --experimental-https --experimental-https-key _wildcard.debate.in-key.pem --experimental-https-cert _wildcard.debate.in.pem -H dev.debate.in -p 443",
    "prepare:cert": "mkcert -install && mkcert *.debate.in",
    "build": "next build",
    "export": "next export",
    "predeploy": "npm run build",
    "deploy": "echo balance-debate.com > out/CNAME && touch out/.nojekyll && gh-pages -d out --dotfiles",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-virtual": "^3.13.10",
    "date-fns": "^4.1.0",
    "gh-pages": "^6.3.0",
    "next": "15.3.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^2.15.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
